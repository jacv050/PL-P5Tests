(* 
	En esta prueba asignaremos una constante integer a un
	array de tipo integer, e imprimiremos esta variable,
	obteniendo por pantalla el caracter escrito anteriormente.

*)

program c;
var r:array [5..10] of array [20..22] of real;
    c:integer;
    b1,b2:BOOLEAN;

begin
   b1 := false;
   b2 := true;
   if(b1 = true) then
   	writeln(b2)
   else if(b1 = true) then
	writeln(b2)
   else
	writeln(b1)     (* Salida: 0 *)
end.
