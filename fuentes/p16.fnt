program prueba;
var
 a:array [0..49] of Integer;
 i:Integer;
begin
  i := 0;
  while i<50 do
   begin
    a[i] := 0;
    i := i+1
   end;

  i := 0;
  while i<10 do
   begin
    a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[i]]]]]]]]]]]]]]]]]] := 
       a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[a[i]]]]]]]]]]]]]]]]]]+5;
    i := i+1
   end;

  writeln(a[0]) (* Salida: 50 *)
end.
